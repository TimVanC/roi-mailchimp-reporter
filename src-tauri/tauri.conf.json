{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ROI Mailchimp Reporter",
  "version": "0.1.0",
  "identifier": "com.roi-mailchimp-reporter.dev",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1421",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ROI Mailchimp Reporter",
        "width": 1200,
        "height": 800,
        "label": "main"
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "dialog": null,
    "shell": null
  }
}
