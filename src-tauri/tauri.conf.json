{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ROI Mailchimp Reporter",
  "version": "1.1.6",
  "identifier": "com.roi.mailchimp-reporter",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ROI Mailchimp Reporter",
        "width": 1200,
        "height": 800,
        "label": "main",
        "center": true,
        "minWidth": 800,
        "minHeight": 600,
        "decorations": true,
        "transparent": false,
        "devtools": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "Â© 2024 ROI-NJ",
    "targets": ["dmg", "app", "msi"],
    "publisher": "ROI-NJ",
    "icon": [
      "icons/roi-icon.png",
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "http://timestamp.digicert.com"
    },
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "10.13",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    }
  },
  "plugins": {
    "dialog": null,
    "shell": {
      "open": true
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/TimVanC/roi-mailchimp-reporter/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDhFOUVGMTUyRjg4RTRDREQKUldUZFRJNzRVdkdlanNCZTUrT1ZyTWk3YXViQUNzWGVTRXhmWDRYdWx2MUMyTmF1Skp4UWs5ZnUK"
    }
  }
}
